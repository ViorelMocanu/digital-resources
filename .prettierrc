{
	"useTabs": true,
	"semi": true,
	"quoteProps": "consistent",
	"trailingComma": "es5",
	"bracketSpacing": true,
	"bracketSameLine": false,
	"arrowParens": "always",
	"embeddedLanguageFormatting": "auto",
	"endOfLine": "lf",
	"requirePragma": false,
	"insertPragma": false,
	"proseWrap": "never",
	"singleQuote": true,
	"singleAttributePerLine": false,
	"printWidth": 180,
	"tabWidth": 4,
	"htmlWhitespaceSensitivity": "css",
	"plugins": ["prettier-plugin-astro"]
}
