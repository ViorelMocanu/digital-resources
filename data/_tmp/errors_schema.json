{
	"$schema": "http://json-schema.org/draft-07/schema#",
	"title": "ErrorsCSV",
	"type": "object",
	"properties": {
		"timestamp": {
			"type": "string",
			"format": "date-time"
		},
		"url": {
			"type": "string"
		},
		"error_message": {
			"type": "string"
		}
	},
	"required": ["timestamp", "url", "error_message"]
}
