		"@typescript-eslint/parser": "^6.7.5",
		"@vite-pwa/astro": "^0.1.3",
		"astro": "^3.2.2",
		"eslint": "^8.51.0",
		"eslint-plugin-astro": "^0.29.1",
		"eslint-plugin-jsdoc": "^46.8.2",
		"eslint-plugin-jsx-a11y": "^6.7.1",
		"eslint-plugin-markdown": "^3.0.1",
		"markdownlint-cli": "^0.37.0",
